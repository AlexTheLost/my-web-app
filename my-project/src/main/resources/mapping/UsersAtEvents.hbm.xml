<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="course.model.data.entity.users_at_events.UsersAtEvents"
        table="users_at_events" catalog="mywebapp">
        <id name="idUsersAtEvents" type="java.lang.Long" access="field">
            <column name="id_users_at_events" />
            <generator class="native" />
        </id>
        <many-to-one name="idEvent"
            class="course.model.data.entity.events.Event" fetch="select">
            <column name="id_event" not-null="true" />
        </many-to-one>
        <many-to-one name="idUser"
            class="course.model.data.entity.users.User" fetch="select">
            <column name="id_user" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>